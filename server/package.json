{
  "name": "health-server",
  "version": "2.0.0",
  "description": "",
  "dependencies": {
    "@michaelkohler/eslint-config-base": "^6.8.18",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.intersection": "^4.4.0",
    "mariadb": "^2.2.0",
    "mqtt": "^3.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.21.4"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.0",
    "@michaelkohler/eslint-config-node": "^6.8.18",
    "@michaelkohler/eslint-config-test": "^6.8.18",
    "ava": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-unicorn": "^17.0.1",
    "nyc": "^15.0.0",
    "sequelize-cli": "^5.5.1",
    "sinon": "^9.0.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "eslint": "eslint .",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "start": "npm run migrate && env DEBUG=\"health*\" node index.js",
    "test": "nyc ava"
  },
  "author": "Michael Kohler <me@michaelkohler.info>",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "statements": 90,
    "branches": 66,
    "functions": 90,
    "lines": 90,
    "include": [
      "lib/**/*.js",
      "middleware/**/*.js",
      "models/**/*.js",
      "routes/**/*.js"
    ],
    "all": true,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "timeout": "10s",
    "babel": {
      "compileAsTests": [
        "tests/**/*.js"
      ]
    }
  }
}
