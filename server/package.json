{
  "name": "health-server",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jwt-simple": "^0.5.6",
    "lodash.intersection": "^4.4.0",
    "mariadb": "^2.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^5.16.0"
  },
  "devDependencies": {
    "@michaelkohler/eslint-config-node": "^6.8.7",
    "ava": "^2.3.0",
    "eslint": "^6.2.1",
    "nyc": "^14.1.1",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.4.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "eslint": "eslint .",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "start": "npm run migrate && env DEBUG=\"health*\" node index.js",
    "test": "nyc ava"
  },
  "author": "Michael Kohler <me@michaelkohler.info>",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 10,
    "statements": 10,
    "functions": 10,
    "branches": 10,
    "include": [
      "middleware/**/*.js",
      "models/**/*.js",
      "routes/**/*.js"
    ],
    "all": true,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "timeout": "2s"
  }
}
